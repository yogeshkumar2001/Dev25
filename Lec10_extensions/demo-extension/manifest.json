{
    "name": "Demo Extension",
    "version": "1.0",
    "description": "This is demo !",
    "manifest_version": 2,
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "css": ["style.css"],
          "js": ["content.js"]
        }
      ],
      "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
      "browser_action": {
        "default_popup": "popup.html"
      },
      "web_accessible_resources": ["images/*"]
  }